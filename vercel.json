{
  "version": 2,
  "builds": [
    {
      "src": "api/streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb",
        "pipArgs": "--no-cache-dir"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/streamlit_app.py"
    }
  ]
}